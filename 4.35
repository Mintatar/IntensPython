n = int(input())
e = int(input())
a = n // 1000
b = n % 1000 // 100
c = n % 100 // 10
d = n % 10
if a + b == c + d:
    print("YES")
else:
    print("NO")
if a + b + c + d % 3 == 0:
    print("YES")
else:
    print("NO")
if a * b * c * d % 4 == 0:
    print("YES")
else:
    print("NO")
if a * b * c * d % e == 0:
    print("YES")
else:
    print("NO")

